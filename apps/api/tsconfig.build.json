{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmit": false,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "strict": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "exactOptionalPropertyTypes": false,
    "module": "CommonJS",
    "target": "ES2020",
    "moduleResolution": "Node"
  },
  "exclude": ["test", "**/*.test.ts", "**/*.spec.ts"]
}
